{
    "manifest_version": 2,

    "name": "Google Form Filler",
    "description": "A chrome extension to autofill Google forms to save time",
    "version": "1.0",

    "browser_action": {
        "default_popup": "popup.js",
        "default_icon": "icon.png"
    },

    "permissions": [
        "activeTab",
        "http://*/*", "https://*/*"
    ]
}