{
    "manifest_version": 2,

    "name": "Google Form Filler",
    "description": "A chrome extension to autofill Google forms to save time",
    "version": "1.0",

    "browser_action": {
        "default_popup": "index.js",
        "default_title": "Click here"
    },

    "permissions": [
        "activeTab",
        "http://*/*", "https://*/*"
    ]
}